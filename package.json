{
	"name": "@khalyomede/fang",
	"description": "Parallelized task runner.",
	"keywords": [
		"gulp",
		"parallelized",
		"task runner",
		"fang",
		"build tool"
	],
	"version": "0.1.5",
	"main": "lib/main.js",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/khalyomede/fang"
	},
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@stryker-mutator/core": "1.*",
		"@types/node": "11.*",
		"autoprefixer": "9.*",
		"chai": "4.*",
		"clean-css": "4.*",
		"coveralls": "3.*",
		"gulp": "4.*",
		"gulp-imagemin": "5.*",
		"gulp-typescript": "5.*",
		"gulp-webp": "4.*",
		"html-minifier": "3.*",
		"imagemin": "6.*",
		"imagemin-cli": "4.*",
		"mocha": "6.*",
		"modify-filename": "1.*",
		"node-sass": "4.*",
		"nyc": "13.*",
		"postcss": "7.*",
		"pug": "2.*",
		"typescript": "3.*",
		"uglify-es": "3.*"
	},
	"dependencies": {
		"cli": "1.*",
		"cli-color": "1.*",
		"commondir": "1.*",
		"fancy-log": "1.*",
		"fs-extra": "7.*",
		"glob": "7.*",
		"performance-now": "2.*",
		"slash": "2.*"
	},
	"bin": {
		"fang": "lib/cli.js"
	},
	"scripts": {
		"start": "gulp watch",
		"build": "gulp build",
		"test": "nyc mocha test",
		"mocha": "mocha",
		"mutate": "stryker run --mutate lib/main.js",
		"coverage": "nyc mocha test && nyc report --reporter=text-lcov | coveralls"
	},
	"files": [
		"lib/main.js",
		"lib/cli.js"
	],
	"nyc": {
		"all": true,
		"include": [
			"lib/**/*.js"
		],
		"exclude": [
			"lib/cli.js",
			"./fang.js"
		]
	}
}
